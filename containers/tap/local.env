
# --- LOCAL ENV CONTEXT --- #
# Keg paths
KEG_PATH=/Users/lancetipton/keg
CLI_PATH=/Users/lancetipton/keg/cli
CORE_PATH=/Users/lancetipton/keg/keg-core
COMPONENTS_PATH=/Users/lancetipton/keg/keg-components
RE_THEME_PATH=/Users/lancetipton/keg/re-theme
TAP_RESOLVER_PATH=/Users/lancetipton/keg/tap-resolver

# Docker / Docker Compose paths
COMPOSE_DEFAULT=/Users/lancetipton/keg/cli/containers/tap/compose-default.yml
COMPOSE_ENV=/Users/lancetipton/keg/cli/containers/tap/compose-local.yml

# The CONTEXT_PATH env should be the location of the tap being run
# So it can not be set inside the .env file
CONTEXT_PATH=INITIAL

CONTEXT_DOCKER_FILE=/Users/lancetipton/keg/cli/containers/tap/Dockerfile
CONTEXT_ENV_FILE=/Users/lancetipton/keg/cli/containers/tap/local.env

# --- DOCKER ENV CONTEXT --- #
DOC_CLI_PATH=/keg/cli
DOC_CORE_PATH=/keg/tap/node_modules/keg-core
DOC_TAP_PATH=/keg/tap

# --- GENERAL CONTEXT ENVs --- #

# Expo data
EXPO_DEBUG_PORT=19002
EXPO_APP_PORT=19006

# Image Build information
IMAGE=tap
VERSION=0.0.1
CONTAINER_NAME=tap

# Git data
GIT_KEY=INITIAL
GIT_CLI_URL=INITIAL
GIT_TAP_URL=INITIAL
