{
  "name": "keg-cli",
  "version": "1.0.0",
  "description": "Commands when working with the Keg",
  "main": "./keg",
  "repository": "https://github.com/lancetipton/Keg-CLI",
  "author": "Lance Tipton",
  "license": "MIT",
  "bin": {
    "keg": "./keg"
  },
  "files": [
    "./keg",
    "src/"
  ],
  "preferGlobal": true,
  "engines": {
    "node": ">= 12.15.0"
  },
  "scripts": {
    "postinstall": "node ./scripts/postinstall/index.js",
    "clean": "yarn clean:lock && yarn cache clean && rimraf node_modules && npm cache clean --force",
    "clean:lock": "rimraf ./yarn.lock && rimraf ./package.lock",
    "keg": "./keg",
    "setup:bash": "bash ./scripts/setup/setup.sh"
  },
  "dependencies": {
    "app-root-path": "2.2.1",
    "axios": "^0.19.2",
    "colors": "1.4.0",
    "inquirer": "7.0.6",
    "jsutils": "git+https://github.com/lancetipton/jsutils.git",
    "load-yaml-file": "0.2.0",
    "module-alias": "2.2.2",
    "recursive-readdir": "2.2.2",
    "rimraf": "2.6.3",
    "simple-git": "1.131.0",
    "spawn-cmd": "git+https://github.com/lancetipton/spawn-cmd.git",
    "sync-glob": "1.4.0",
    "tap-resolver": "git+https://github.com/simpleviewinc/tap-resolver.git",
    "tree-kill": "1.2.2",
    "write-yaml-file": "4.0.0"
  },
  "devDependencies": {},
  "_moduleAliases": {
    "KegConst": "src/constants",
    "KegCrypto": "src/libs/crypto",
    "KegDocCli": "src/libs/docker",
    "KegFileSys": "src/libs/fileSys",
    "KegLibs": "src/libs",
    "KegLog": "src/libs/logger",
    "KegMocks": "__mocks__",
    "KegProc": "src/libs/process",
    "KegPackage": "package.json",
    "KegQuestions": "src/libs/questions",
    "KegRoot": ".",
    "KegRunTask": "src/runTask",
    "KegScripts": "scripts",
    "KegSrc": "src",
    "KegTasks": "src/tasks",
    "KegUtils": "src/utils/"
  }
}
