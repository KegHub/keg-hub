{
  "name": "@svkeg/ask-it",
  "version": "0.0.1",
  "description": "Node CLI helper for asking questions",
  "main": "src/index.js",
  "repository": {
    "url": "git+https://github.com/simpleviewinc/keg-hub.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/simpleviewinc/keg-hub/issues"
  },
  "homepage": "https://github.com/simpleviewinc/keg-hub#readme",
  "author": "Lance Tipton",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 12.15.0"
  },
  "bin": {
    "askIt": "./src/askIt"
  },
  "files": [
    "src/"
  ],
  "scripts": {
    "validate": "node ./scripts/validate.js",
    "test": "NODE_ENV=test jest",
    "test:reports": "yarn test --coverage && yarn view:reports"
  },
  "preferGlobal": true,
  "dependencies": {
    "@svkeg/jsutils": "0.0.1",
    "colors": "1.4.0",
    "inquirer": "7.0.6"
  },
  "devDependencies": {
    "jest": "24.9.0",
    "jest-cli": "24.8.0"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "globals": {
      "__DEV__": true
    },
    "moduleNameMapper": {},
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)"
    ],
    "transformIgnorePatterns": [
      ".*"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!**/__mocks__/**/*.{js}"
    ],
    "coverageDirectory": "reports/coverage",
    "setupFilesAfterEnv": [
      "<rootDir>/scripts/setupTests.js"
    ]
  }
}
