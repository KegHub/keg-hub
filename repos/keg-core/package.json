{
  "name": "@keg-hub/keg-core",
  "main": "node_modules/expo/AppEntry.js",
  "description": "A framework for developing cross-platform react apps, that is preconfigured to target both web, desktop, and mobile architectures.",
  "version": "9.5.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "git+https://github.com/keg-hub/keg-hub.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/keg-hub/keg-hub/issues"
  },
  "homepage": "/",
  "license": "MIT",
  "scripts": {
    "and": "yarn android",
    "android": "EXPO_DEBUG=true NODE_ENV=development PLATFORM=native expo start --clear --android --dev",
    "build": "echo \"Keg-Core build!\"",
    "build:android": "NODE_ENV=production PLATFORM=native expo build:android",
    "build:ios": "NODE_ENV=production PLATFORM=native expo build:ios",
    "build:optimize": "NODE_ENV=production PLATFORM=web npx expo-optimize",
    "build:web": "yarn clean:web:build; yarn build:optimize; NODE_ENV=production PLATFORM=web expo build:web --clear --no-pwa",
    "clean": "yarn clean:cache && yarn clean:nm",
    "clean:babel": "rimraf ./node_modules/.cache/@babel",
    "clean:cache": "yarn clean:rn && yarn clean:babel; yarn cache clean",
    "clean:dev": "yarn clean:rn; yarn clean:babel; yarn clean:web:build; yarn clean:nm",
    "clean:full": "rimraf yarn.lock && yarn clean:dev",
    "clean:install": "yarn clean:full; yarn install --force",
    "clean:nm": "rimraf yarn.lock; rimraf package.lock; rimraf node_modules",
    "clean:rn": "yarn cache clean && watchman watch-del-all && rimraf $TMPDIR/react-* && rimraf $TMPDIR/metro-bundler-cache-* && rimraf $TMPDIR/metro-cache/*",
    "clean:web:build": "rimraf ./web-build",
    "eslint:watch": "onchange '**/*.{js,jsx}' -- eslint --config ./core/configs/eslintrc.config.js {{changed}} --fix",
    "format:eslint": "eslint --config ./core/configs/eslintrc.config.js . --fix --quiet",
    "format:prettier": "prettier --config ./core/configs/prettier.config.js --ignore-path .eslintignore --write '**/*.{js,jsx}'",
    "format": "yarn format:prettier && yarn format:eslint",
    "ios": "EXPO_DEBUG=true NODE_ENV=development PLATFORM=native expo start --clear --ios --dev",
    "postinstall": "node ./core/scripts/js/postInstall.js",
    "roll:dev": "PLATFORM=web rollup -c ./core/configs/rollup.config.js",
    "start": "NODE_ENV=development expo start",
    "tap:start": "nodemon --watch ../../package.json --watch ./package.json --watch ../../configs --watch ./core/configs  --exec 'EXPO_DEBUG=true NODE_ENV=development PLATFORM=web expo start --clear --web --dev'",
    "test": "PLATFORM=web NODE_ENV=test jest --no-cache --config=./core/configs/jest.config.js --maxWorkers=50%",
    "test:reports": "yarn test --coverage && yarn view:reports",
    "view:reports": "open reports/test-results.html && open reports/coverage/lcov-report/index.html",
    "web": "nodemon --watch ./core/configs --watch ./package.json --exec 'EXPO_DEBUG=true NODE_ENV=development PLATFORM=web expo web --web --dev'",
    "web:build": "yarn build:web",
    "print:envs": "node core/scripts/js/printKegEnvs.js ./"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-transform-destructuring": "7.16.7",
    "@babel/plugin-transform-object-assign": "7.16.7",
    "@babel/plugin-transform-parameters": "7.16.7",
    "@babel/plugin-transform-react-jsx-source": "7.16.7",
    "@babel/preset-env": "7.16.7",
    "@babel/preset-react": "7.16.7",
    "@babel/runtime": "7.16.7",
    "@expo/webpack-config": "0.16.15",
    "@keg-hub/jsutils": "8.7.0",
    "@keg-hub/keg-components": "9.5.0",
    "@keg-hub/re-theme": "9.5.0",
    "@keg-hub/tap-resolver": "9.0.0",
    "@testing-library/react-hooks": "7.0.2",
    "app-root-path": "3.0.0",
    "axios": "0.24.0",
    "@babel/eslint-parser": "7.16.5",
    "babel-jest": "27.4.5",
    "babel-loader": "8.2.3",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-preset-expo": "9.0.2",
    "enzyme": "3.11.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.6",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.6.0",
    "eslint-plugin-jest": "25.3.4",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-native": "4.0.0",
    "expo": "44.0.3",
    "expo-secure-store": "11.1.0",
    "expo-linear-gradient": "11.0.0",
    "fs-extra": "10.0.0",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "jest-expo": "44.0.1",
    "jest-html-reporter": "3.4.2",
    "lint-staged": "12.1.5",
    "nodemon": "2.0.15",
    "onchange": "7.1.0",
    "prettier": "2.5.1",
    "prop-types": "15.8.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.66.4",
    "react-native-screens": "3.10.1",
    "react-native-svg": "12.1.1",
    "react-native-svg-web": "1.0.9",
    "react-native-web": "0.17.5",
    "react-redux": "7.2.6",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-router-native": "5.2.0",
    "react-test-renderer": "17.0.2",
    "redux": "4.1.2",
    "redux-devtools-extension": "2.13.9",
    "rimraf": "3.0.2",
    "rollup": "2.62.0"
  },
  "resolutions": {
    "@keg-hub/jsutils": "8.7.0",
    "@keg-hub/keg-components": "9.5.0",
    "@keg-hub/re-theme": "9.5.0",
    "@keg-hub/tap-resolver": "8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-test-renderer": "17.0.2",
    "ast-types": "0.14.2"
  },
  "nodemonConfig": {
    "ext": "js",
    "ignore": [
      "/keg/tap/src",
      "/keg/tap/node_modules/*",
      "/keg/tap/node_modules/keg-core/node_modules/*",
      "/keg/tap/node_modules/keg-core/core"
    ],
    "watch": [
      "/keg/tap/configs",
      "/keg/tap/package.json",
      "/keg/tap/node_modules/keg-core/package.json",
      "/keg/tap/node_modules/keg-core/core/configs"
    ],
    "delay": "2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --config ./core/configs/lint-staged.config.js"
    }
  }
}
